{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "SupabaseConnection": "<supabase-connection-string>"
  },
  "JWT": {
    "Issuer": "https://<supabase-project-id>.supabase.co/auth/v1",
    "Secret": "<supabase-project-jwt-secret>",
    "Audience": "authenticated",
    "ExpiryInMinutes": 60
  }
}
